{
	"editor.codeActionsOnSave": [
		"source.addMissingImports",
		"source.fixAll",
		"source.organizeImports"
	],
	"editor.defaultFormatter": "oxc.oxc-vscode",
	"editor.foldingImportsByDefault": false,
	"editor.formatOnSave": true,
	"editor.tabSize": 2,
	"files.exclude": {},
	"files.watcherExclude": {
		"**/build": true
	},
	"files.associations": {
		"*.css": "tailwindcss"
	}
}